<%= form_for @item, url: url do |f| %>
  <%= f.hidden_field :user_id, value: @list.user.id %>
  <%= f.hidden_field :purchased, value: false %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <br /><br />

  <%= f.label :quantity %>
  <%= f.number_field :quantity %>

  <br /><br />

  <%= f.submit %>
<% end %>
